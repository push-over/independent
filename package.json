{
	"name": "nest-template",
	"version": "1.0.1",
	"description": "Nestjs GraphQL Prisma",
	"author": "Praise",
	"license": "MIT",
	"keywords": [
		"nestjs",
		"bootstrap",
		"express",
		"apollo-graphql",
		"prisma"
	],
	"scripts": {
		"build": "tsc -p tsconfig.build.json",
		"format": "prettier --write \"src/**/*.ts\"",
		"start": "ts-node -r tsconfig-paths/register src/main.ts",
		"start:dev": "nodemon",
		"start:debug": "nodemon --config nodemon-debug.json",
		"prestart:prod": "rm -rf dist && tsc",
		"start:prod": "NODE_ENV=production node -r ./tsconfig-paths-bootstrap.js dist/main.js",
		"prisma:save": "npx prisma migrate save --experimental",
		"prisma:up": "npx prisma migrate up --experimental",
		"prisma:down": "npx prisma migrate down --experimental",
		"prisma:studio": "npx prisma studio",
		"prisma:generate": "npx prisma generate",
		"seed": "ts-node prisma/seed.ts",
		"prisma:generate:watch": "npx prisma generate --watch",
		"start:db": "npm run prisma:up && npm run prisma:generate",
		"postinstall": "npm run prisma:generate",
		"gen": "ts-node src/generator/generate-typings.ts",
		"lint": "tslint -p tsconfig.json -c tslint.json",
		"lint:fix": "tslint --fix -p tsconfig.json -c tslint.json",
		"test": "NODE_ENV=testing jest -w 1 --config ./jest.config.json --forceExit",
		"test:watch": "NODE_ENV=testing jest -w 1 --watch --forceExit",
		"test:cov": "NODE_ENV=testing jest -w 1 --config ./jest.config.json --coverage --forceExit",
		"test:debug": "NODE_ENV=testing node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest -w 1 --runInBand --forceExit",
		"test:e2e": "NODE_ENV=testing jest -w 1 --config ./test/jest-e2e.json --forceExit",
		"webpack": "webpack --config webpack.config.js",
		"start:hmr": "node dist/main.js",
		"doc": "npx compodoc --port 11045 -p tsconfig.json -s",
		"doc:serve": "yarn doc && npx compodoc -s",
		"docker:migrate": "docker-compose -f docker-compose.migrate.yml up -d",
		"docker:db": "docker-compose -f docker-compose.db.yml up -d",
		"docker:seed": "docker exec -it nest-api npm run seed",
		"docker": "docker-compose up -d",
		"docker:build": "docker-compose build"
	},
	"dependencies": {
		"@apollo/federation": "^0.20.4",
		"@apollo/gateway": "^0.21.1",
		"@devoxa/prisma-relay-cursor-connection": "^1.1.0",
		"@nestjs/bull": "^0.3.0",
		"@nestjs/common": "^7.5.3",
		"@nestjs/core": "^7.5.3",
		"@nestjs/graphql": "^8.0.0",
		"@nestjs/platform-express": "^7.5.3",
		"@nestjs/schedule": "^0.4.1",
		"@prisma/client": "^2.11.0",
		"apollo-server": "^2.19.0",
		"apollo-server-cache-memcached": "^0.6.6",
		"apollo-server-express": "^2.19.0",
		"apollo-server-plugin-response-cache": "^0.5.6",
		"assert": "^2.0.0",
		"axios": "^0.21.0",
		"bcryptjs": "^2.4.3",
		"bull": "^3.18.1",
		"cache-manager": "^3.4.0",
		"chai": "^4.2.0",
		"class-transformer": "^0.3.1",
		"class-validator": "^0.12.2",
		"cloudinary": "1.23.0",
		"compression": "^1.7.4",
		"cron": "^1.8.2",
		"csurf": "^1.11.0",
		"dateformat": "^3.0.3",
		"dotenv": "^8.2.0",
		"express-rate-limit": "^5.1.3",
		"fs": "0.0.2",
		"google-translate": "^3.0.0",
		"graphql": "^15.4.0",
		"graphql-depth-limit": "^1.1.0",
		"graphql-rate-limit": "^2.0.1",
		"graphql-tools": "^9.0.1",
		"graphql-type-json": "^0.3.2",
		"graphql-voyager": "^1.0.0-rc.31",
		"handlebars": "^4.7.6",
		"helmet": "^4.2.0",
		"ioredis": "^4.19.2",
		"jest-config": "^28.0.0",
		"jsonwebtoken": "^8.5.1",
		"merge-graphql-schemas": "^1.7.8",
		"mongodb": "^3.6.3",
		"morgan": "^1.10.0",
		"multer": "^1.4.2",
		"nodemailer": "^6.4.16",
		"nodemailer-express-handlebars": "^4.0.0",
		"passport": "^0.4.1",
		"passport-facebook": "^3.0.0",
		"passport-facebook-token": "^4.0.0",
		"passport-google-oauth": "^2.0.0",
		"passport-google-plus-token": "^2.1.0",
		"passport-google-token": "^0.1.2",
		"reflect-metadata": "^0.1.13",
		"rimraf": "^3.0.2",
		"rxjs": "^6.6.3",
		"stripe": "^8.122.1",
		"tsconfig-paths": "^3.9.0",
		"typescript": "^4.0.5",
		"uuid": "^8.3.1",
		"webpack-bundle-analyzer": "^4.1.0",
		"winston": "^3.3.3"
	},
	"devDependencies": {
		"@compodoc/compodoc": "^1.1.11",
		"@nestjs/testing": "^7.5.3",
		"@prisma/cli": "^2.11.0",
		"@types/bull": "^3.14.4",
		"@types/express": "^4.17.9",
		"@types/jest": "^26.0.15",
		"@types/mongodb": "^3.5.33",
		"@types/node": "^14.14.8",
		"@types/nodemailer": "^6.4.0",
		"@types/stripe": "^7.13.25",
		"@types/supertest": "^2.0.10",
		"codecov": "^3.8.1",
		"concurrently": "^5.3.0",
		"coveralls": "^3.1.0",
		"husky": "^4.3.0",
		"jest": "^26.6.3",
		"lint-staged": "^10.5.1",
		"nodemon": "^2.0.6",
		"prettier": "^2.1.2",
		"prisma-dbml-generator": "^0.3.1",
		"progress-bar-webpack-plugin": "^2.1.0",
		"supertest": "^6.0.1",
		"ts-jest": "^26.4.4",
		"ts-loader": "^8.0.11",
		"ts-node": "^9.0.0",
		"tslint": "^6.1.3",
		"tslint-clean-code": "^0.2.10",
		"tslint-config-prettier": "^1.18.0",
		"wait-on": "^5.2.0",
		"webpack": "^5.5.0",
		"webpack-cli": "^4.2.0",
		"webpack-loader-helper": "^1.0.0",
		"webpack-node-externals": "^2.5.2",
		"yarn": "^1.22.10"
	},
	"lint-staged": {
		"*.ts": [
			"yarn format",
			"yarn lint:fix",
			"git add"
		]
	},
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged"
		}
	}
}
